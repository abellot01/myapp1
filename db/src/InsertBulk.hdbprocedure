procedure "InsertBulk"()
language sqlscript
sql security invoker
as
begin
    declare i int;
    i = 0;
    for i in 1..20000
        do 
            insert into myapp1_DB_TESTTABLE select top 1 sysuuid||sysuuid from "myapp1_DB_OBJECTS" cross join "myapp1_DB_OBJECTS";
    end for;
    delete from myapp1_DB_TESTTABLE;
end